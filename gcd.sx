function gcd(a,b) {
  if (b == 0) { a; }
  else { gcd(b, a % b); };
}

suite small_tests {
  a = symbolic();
  b = symbolic();
  assume(a > 0);
  assume(b > 0);
  r = gcd(a,b);
  test gcd_divides_arguments {
    check(a % r == 0);
    check(b % r == 0);
  }
  test gcd_is_greatest {
    c = symbolic();
    assume(c > 0);
    assume(a % c == 0);
    assume(b % c == 0);
    check(c <= r);
  }
}
