function gcd(a,b) {
  if (b == 0) { a; }
  else { gcd(b, a % b); };
}

suite {
  a = symbolic();
  b = symbolic();
  assume(a < 200);
  assume(b < 200);
  r = gcd(a,b);
  test {
    assert(a % r == 0);
    assert(b % r == 0);
  }
  test {
    c = symbolic();
    assume(a % c == 0);
    assume(b % c == 0);
    assert(c <= r);
  }
}